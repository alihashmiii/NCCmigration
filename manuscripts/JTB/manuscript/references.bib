journal = {Cell Reports},
month = {may},
number = {9},
pages = {2076--2088},
pmid = {19546857},
publisher = {Elsevier},
title = {{Leader cells define directionality of trunk, but not cranial, neural crest cell migration}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S2211124716305113},
volume = {15},
year = {2016}
