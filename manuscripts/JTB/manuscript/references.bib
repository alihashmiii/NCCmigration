issn = {1477-9129},
journal = {Development},
keywords = {chick,cranial cell migration,laser capture microdissection,mathematical modeling,neural crest,numerical,qpcr},
month = {aug},
number = {16},
pages = {2935--44},
pmid = {22764050},
publisher = {Oxford University Press for The Company of Biologists Limited},
title = {{Multiscale mechanisms of cell migration during development: theory and experiment.}},
